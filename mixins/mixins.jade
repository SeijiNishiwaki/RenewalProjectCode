mixin local-nav()
	ul.nav.nav-pills.nav-pills-custome.local-nav
		li(role="navigation"): a.local-nav-left-link(href="/overview") ABOUT
		li(role="navigation"): a.local-nav-left-link(href="/team") TEAM
		li(role="navigation"): a.local-nav-left-link(href="/jobs") JOBS
		li(role="navigation"): a.local-nav-left-link(href="/press") PRESS
		li(role="navigation"): a.local-nav-left-link(href="#") FAQ
		li.pull-right(role="navigation"): a.local-nav-right-link(href="/about/dev") BUSINESS INFORMATION
mixin about-local-nav(page)
				if (page == "Developments")

					ul.nav.nav-pills.nav-pills-custome
						//
							li(role="navigation"): a(href="/about") OVERVIEW
						li(role="navigation"): a.blue(href="/about/dev") DEVELOPMENTS
						li(role="navigation"): a(href="/about/management") MANAGEMENT
						li(role="navigation"): a(href="/consultation") CONSULTATION
						//
							li(role="navigation"): a(href="/about/contact") CONTACT
				if (page == "Management")

					ul.nav.nav-pills.nav-pills-custome
						//
							li(role="navigation"): a(href="/about") OVERVIEW
						li(role="navigation"): a(href="/about/dev") DEVELOPMENTS
						li(role="navigation"): a.blue(href="/about/management") MANAGEMENT
						li(role="navigation"): a(href="/consultation") CONSULTATION
						//
							li(role="navigation"): a(href="/about/contact") CONTACT
				if (page == "Consultation")

					ul.nav.nav-pills.nav-pills-custome
						//
							li(role="navigation"): a(href="/about") OVERVIEW
						li(role="navigation"): a(href="/about/dev") DEVELOPMENTS
