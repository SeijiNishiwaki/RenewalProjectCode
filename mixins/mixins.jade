mixin m-media-A(imgName,imgLinkPath,alt,label,title,subTitle,text,linkText,linkPath)
	.m-media-wrapper
		.m-media.m-media-A
			if imgName
				if imgLinkPath
					a.media-image-link(href="#{imgLinkPath}")
						img.media-image.img-responsive(src="../images/photos/#{imgName}",alt="#{alt}")
				else
					img.media-image.img-responsive(src="../images/photos/#{imgName}",alt="#{alt}")
			if label
				p.media-label #{label}
			if title
				h3.media-title #{title}
			if subTitle
				p.media-sub-title #{subTitle}
			if text
				p.hidden-xs.media-text #{text}
			if linkText
				a.media-link(href="#{linkPath}")
					| #{linkText}



mixin m-media(imgName,imgLinkPath,alt,title,caption,subTitle,text,linkText,linkPath)
