							li(role="navigation"): a(href="/about/contact") CONTACT
mixin m-section-header-center(title,text)
	.row.m-section-header.text-center
		if title
			.section-header-title.col.col-lg-12.col-md-12.col-sm-12.col-xs-12
				h2 #{title}
		if text
			.section-header-text.col.col-lg-10.col-md-12.col-sm-12.col-xs-12.col-lg-offset-1
				p #{text}
mixin m-section-header-center-link(title,text,linkText,linkPath)
	.row.m-section-header.text-center
		if title
			.section-header-title.col.col-lg-12.col-md-12.col-sm-12.col-xs-12
				h2 #{title}
		if text
			.section-header-text.col.col-lg-10.col-md-12.col-sm-12.col-xs-12.col-lg-offset-1
				p #{text}
					if linkText
						a(href="#{linkPath}") #{linkText}

mixin m-section-header(title,link,text,linkText,linkPath)
	.row.m-section-header
		if title
			.section-header-title.col.col-lg-12.col-md-12.col-sm-12.col-xs-12
				h2.pull-left #{title}
				if linkText
					div.pull-right.text-right.hidden-xs
						a(href="#{linkPath}") #{linkText}
							span.glyphicon.glyphicon-menu-right
		if text
			.section-header-text.col.col-lg-12.col-md-12.col-sm-12.col-xs-12
				p #{text}
		if linkText
			.section-header-link.col.col-lg-6.col-md-6.col-sm-6.col-xs-12.hidden-lg.hidden-md.hidden-sm
				a(href="#{linkPath}") #{linkText}
					span.glyphicon.glyphicon-menu-right
