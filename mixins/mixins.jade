mixin m-media-nearby(imgName,linkPath,alt,brand,name,time,subTitle,price,linkText,linkPath)
	.m-media-wrapper
		.m-media.m-media-nearby
			if imgName
				if linkPath
					a(href="#{linkPath}")
						img.media-image.img-responsive(src= imgName ,alt="#{alt}")
				else
					img.media-image.img-responsive(src= imgName ,alt="#{alt}")
			ul.list-unstyled.media-list
				if brand
					p.media-brand #{brand}
						if time
							span.media-time.pull-right #{time}
				if name
					li: h3: a.media-title.media-name.media-link(src="#{linkPath}") #{name}
				if subTitle
					li: p.media-sub-title #{subTitle}
				if price
					li: p.media-price #{price}
				if linkText
					li: a.media-link(href="#{linkPath}")
						| #{linkText}

mixin m-compare-media(before,after,title,caption,subTitle,text,linkText,linkPath)
	.m-media-wrapper
		.m-media.m-compare-media
			p.blue.text-center Drag to see before and after
			.compare
				if before
					<img class="img-responsive" src="../images/photos/#{before}" />
				if after
					<img class="img-responsive" src="../images/photos/#{after}" />
			if caption === true
				if title
					h3.media-caption #{title}
			else
				if title
					h3.media-title #{title}
			if subTitle
				p.media-sub-title #{subTitle}
			if text
				p.media-text.media-text #{text}
			if linkText
				a.media-link(href="#{linkPath}")
					| #{linkText}
mixin m-compare-media-B(before,after,title,caption,subTitle,text,linkText,linkPath)
	.m-media-wrapper
		.m-media.m-compare.row
			if before
				.col.col.col-lg-6.col-md-6.col-sm-6.col-xs-6
					<img class="img-responsive" src="../images/photos/#{before}" />
			if after
				.col.col.col-lg-6.col-md-6.col-sm-6.col-xs-6
					<img class="img-responsive" src="../images/photos/#{after}" />
			.col.col.col-lg-12.col-md-12.col-sm-12.col-xs-12
				if caption === true
					if title
						h3.media-caption #{title}
				else
					if title
						h3.media-title #{title}
				if subTitle
					p.media-sub-title #{subTitle}
				if text
					p.media-text #{text}
				if linkText
					a.media-link(href="#{linkPath}")
						| #{linkText}
